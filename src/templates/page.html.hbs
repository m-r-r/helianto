{{> head.html}}
<main class="page__body">
    {{#if page}}
      <h1>{{page.title}}</h1>
      {{{page.content}}}
    {{else}}
      {{#each pages}}
      <section class="post">
        <h3 class="post__title">
          <a href="{{url}}">{{title}}</a>
        </h3>
      </section>
      {{/each}}
    {{/if}}
</main>
{{> foot.html}}
